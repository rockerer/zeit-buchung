{
    "name": "tausendstel-licht/zeit-buchung",
    "description": "Console tool to record working time.",
    "type": "project",
    "require": {
        "symfony/console": "^4.3",
        "php": "^7.2",
        "ext-json": "*",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-config": "^3.3"
    },
    "authors": [
        {
            "name": "Lukas-Hubert von Zukowski",
            "email": "tausendstel.licht@gmail.com"
        }
    ],
    "bin": [
        "bin/zeit-buchung.php"
    ],
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ZeitBuchung\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
